    "def compound_interest(principle, rate, time): \n",
    "  \n",
    "    # Calculates compound interest  \n",
    "    CI = principle * (pow((1 + rate / 100), time)) \n",
    "    print(\"Compound interest is\", CI) \n",
    "  \n",
    "compound_interest(330000, 10.25, 5) "
   ]
